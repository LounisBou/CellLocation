{
    "name": "lounisbou/cell-location",
    "description": "CellLocation uses API to retrieve estimation of the position of a mobile within a cell's coverage area.",
    "type": "library",
    "require": {
        "symfony/dotenv": "^7.1"
    },
    "require-dev": {
        "pestphp/pest": "^3.0"
    },
    "license": "Apache 2.0",
    "autoload": {
        "psr-4": {
            "Lounisbou\\CellLocation\\": "src/"
        }
    },
    "authors": [
        {
            "name": "LounisBou",
            "email": "lounis.bou@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
